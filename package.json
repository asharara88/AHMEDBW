    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.6",
    "eslint-plugin-react-refresh": "^0.4.20",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.344.0",
    "openai": "^4.100.0",
