    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "xml2js": "^0.6.2",
    "node-expat": "npm:@xml-tools/parser@^1.0.11"
